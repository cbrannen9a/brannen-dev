{
	"name": "brannen-dev",
	"private": true,
	"description": "Portfolio blog",
	"version": "0.1.0",
	"author": "Chris Brannen <chris@brannen.dev>",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.15",
		"@fortawesome/free-brands-svg-icons": "^5.7.2",
		"@fortawesome/free-regular-svg-icons": "^5.7.2",
		"@fortawesome/free-solid-svg-icons": "^5.7.2",
		"@fortawesome/react-fontawesome": "^0.1.4",
		"@sanity/block-content-to-react": "^2.0.6",
		"firebase": "^5.8.4",
		"gatsby": "^2.1.19",
		"gatsby-image": "^2.0.30",
		"gatsby-plugin-manifest": "^2.0.20",
		"gatsby-plugin-offline": "^2.0.24",
		"gatsby-plugin-react-helmet": "^3.0.7",
		"gatsby-plugin-sharp": "^2.0.23",
		"gatsby-remark-images": "^3.0.5",
		"gatsby-remark-prismjs": "^3.2.4",
		"gatsby-remark-responsive-iframe": "^2.0.9",
		"gatsby-source-filesystem": "^2.0.23",
		"gatsby-source-sanity": "^1.1.2",
		"gatsby-transformer-json": "^2.1.8",
		"gatsby-transformer-remark": "^2.3.0",
		"gatsby-transformer-sharp": "^2.1.15",
		"prismjs": "^1.15.0",
		"prop-types": "^15.7.2",
		"rbx": "^2.1.0",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-helmet": "^5.2.0"
	},
	"devDependencies": {
		"babel-jest": "^24.1.0",
		"babel-preset-gatsby": "^0.1.8",
		"cypress": "^3.1.5",
		"cypress-testing-library": "^2.3.6",
		"eslint-plugin-cypress": "^2.2.1",
		"husky": "^1.3.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.1.0",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"react-test-renderer": "^16.8.4",
		"react-testing-library": "^6.0.0"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"semi": [
				2,
				"always"
			],
			"quotes": [
				2,
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop -o",
		"format": "prettier --write src/**/*.{js,jsx}",
		"start": "GATSBY_GRAPHQL_IDE=playground yarn develop",
		"serve": "gatsby serve",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest -- watch",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"deploy": "yarn build && firebase deploy --only hosting"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cbrannen9a/brannen-dev"
	},
	"bugs": {
		"url": "https://github.com/cbrannen9a/brannen-dev/issues"
	}
}
